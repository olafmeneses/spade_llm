[run]
source = spade_llm
omit = 
    */tests/*
    */test_*
    */.*
    */venv/*
    */.venv/*
    */site-packages/*
    setup.py
    spade_llm/__main__.py
    spade_llm/version.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
